<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.financial.manage.mapper.ContentTypeMapper">
    <select id="getContentTypeBySubfix" resultType="com.financial.manage.entity.ContentType">
        SELECT `subfix`, `content_type`, `can_view` FROM `mi_content_type` WHERE `subfix` = #{subfix} LIMIT 1
    </select>
</mapper>
